            <zeroOrMore>
               <ref name="model.global"/>
            </zeroOrMore>
      
            <oneOrMore>
               <group>
                  <ref name="titlePart"/>
        
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
        
               </group>
            </oneOrMore>
         </group>
         <ref name="att.global.attributes"/>
         <ref name="att.canonical.attributes"/>
         <empty/>
